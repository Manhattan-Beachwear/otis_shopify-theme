        "text_PYkjxj": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h2>PRO-FORM ARCH: COMFORT AND SUPPORT LIKE A RUNNING SHOE</h2>",
            "width": "fit-content",
            "max_width": "normal",
            "alignment": "left",
            "type_preset": "rte",
