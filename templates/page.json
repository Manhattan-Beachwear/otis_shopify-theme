            "accordion_row_HtAKVC": {
              "type": "_accordion-row",
              "settings": {
                "heading": "Replacement service!? How does that work?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_jPJDXm": {
                  "type": "text",
                  "settings": {
                    "text": "<p>Simply head to our <a href=\"https://au.otiseyewear.com/pages/repair-request-form\" title=\"Repair Request\">replacement service form, </a>fill in the details required, attach images requested and our customer service team will reach out within two business days to provide you with a unique 45% off discount code to use on our OTIS website. This code does not expire and cannot be used in conjunction with any other offer. </p>",
                    "width": "100%",
                    "max_width": "normal",
                    "alignment": "left",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
                    "font_size": "1rem",
