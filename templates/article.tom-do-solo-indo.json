        "text_tMKtEJ": {
          "type": "text",
          "name": "t:names.text",
          "settings": {
            "text": "<p>A deceivingly uncrowded Desert Point. The crowds there were intense, with over 100 people in the water at times.  It made me pretty keen to go and chase some empty waves on my own.</p>",
            "width": "fit-content",
            "max_width": "normal",
            "alignment": "left",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
            "line_height": "normal",
            "letter_spacing": "normal",
