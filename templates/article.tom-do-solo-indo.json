        "text_erngTi": {
          "type": "text",
          "name": "t:names.text",
          "settings": {
            "text": "<p>Back on the road in Sumba. Out here, there is limited signal, and even if you do get service, Google Maps is extremely unreliable. To travel, you must speak Indo, and trust in the directions of locals.</p>",
            "width": "fit-content",
            "max_width": "normal",
            "alignment": "left",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
